PACKAGE_NAME="kylin-kmre-modules-dkms"
PACKAGE_VERSION="1.0"
CLEAN="make -C ashmem clean"
MAKE[0]="'make' -j$parallel_jobs -C ashmem KERNELRELEASE=${kernelver} KERNEL_SRC=$kernel_source_dir &&  make -j$parallel_jobs -C virtwifi KERNELRELEASE=${kernelver} KERNEL_SRC=$kernel_source_dir"
BUILT_MODULE_NAME[0]="kmre-ashmem"
BUILT_MODULE_LOCATION[0]="ashmem"
DEST_MODULE_LOCATION[0]="/updates/dkms"
BUILT_MODULE_NAME[1]="kmre-virtwifi"
BUILT_MODULE_LOCATION[1]="virtwifi"
DEST_MODULE_LOCATION[1]="/updates/dkms"
AUTOINSTALL="yes"
REMAKE_INITRD="false"
